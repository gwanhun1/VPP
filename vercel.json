{
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "installCommand": "corepack enable && corepack yarn --version && corepack yarn install --immutable",
  "buildCommand": "corepack yarn build",
  "outputDirectory": "apps/web/dist",
  "framework": "vite",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
